{
  "files.associations": {
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html"
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "[django-html]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "css.validate": true,
  "css.lint.validProperties": [
    "composes"
  ],
  "html.validate.styles": false,
  "django-html.validate.styles": false
}